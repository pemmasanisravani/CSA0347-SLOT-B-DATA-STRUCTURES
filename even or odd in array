#include<stdio.h>
#include<conio.h>
int main()
{
int arr[6]={1,2,3,4,5,6};
int odd,even;
int i;
printf("\nthe even numbers are:");
for(i=0;i<6;i++)
{
if(arr[i]%2==0)
{
printf("%d",arr[i]);
}
}
printf("\n the odd numbers are:");
for(i=0;i<6;i++)
{
if(arr[i]%2==1)
{
printf("%d",arr[i]);
}
}
getch();
return 0;
}
