#include<stdio.h>
#include<conio.h>
int main()
{
int i,j,k,a[2][2],b[2][2],mul[2][2]={0};
printf("enter the a matrix:");
for(i=0;i<2;i++)
  for(j=0;j<2;j++)
    scanf("%d",&a[i][j]);
 printf("enter the b matrix:");
for(i=0;i<2;i++)
  for(j=0;j<2;j++)
    scanf("%d",&b[i][j]);
 printf("multiplication the two  matrix:");
for(i=0;i<2;i++)
  for(j=0;j<2;j++)
     for(k=0;k<2;k++)
    mul[i][j]=mul[i][j]* a[i][k] * b[j][k];
  printf("print the result");
for(i=0;i<2;i++)
  for(j=0;j<2;j++)
    printf("%d",mul[i][j]);
 getch();
 return 0;
 }
