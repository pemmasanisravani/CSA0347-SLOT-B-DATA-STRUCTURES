#include<stdio.h>
##include<conio.h>
int main()
{
int a[2][2],b[2][2],sum[2][2],i,j;
printf("enter the a matrix");
for(i=0;i<2;i++)
     for(j=0;j<2;j++)
	scanf("%d",&a[i][j]);
printf("enter the b matrix");
for(i=0;i<2;i++)
     for(j=0;j<2;j++)
	scanf("%d",&b[i][j]);
printf("addition to two matrix");
for(i=0;i<2;i++)
     for(j=0;j<2;j++)
	sum[i][j]=a[i][j] + b[i][j];
printf("print the result");
for(i=0;i<2;i++)
    for(j=0;j<2;j++)
     printf("%d",sum[i][j]);
getch();
return 0;
}
